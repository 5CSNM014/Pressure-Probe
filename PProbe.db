# --- 起動時に各ピン初期値を読む longin --- 

record(longin, "In2Init") {
    field(DTYP, "stream")
    field(INP,  "@PProbe.proto readP2 L0")
    field(PINI, "YES")
}

record(longin, "In2Evt") {
    field(DTYP, "stream")
    field(INP,  "@PProbe.proto chgInput2 L0")
    field(SCAN, "I/O Intr")
}

record(longout, "In2State") {
    field(DTYP, "Soft Channel")
}

record(calcout, "In2Init2State") {
    field(INPA, "In2Init CP")
    field(CALC, "A")
    field(OUT,  "In2State PP")
}
 
record(calcout, "In2Evt2State") {
    field(INPA, "In2Evt CP")
    field(CALC, "A")
    field(OUT,  "In2State PP")
}

